
@page "/"

<h1>Hello, world!</h1>

Welcome to your new app.

<SurveyPrompt Title="How is Blazor working for you?" />

<AuthorizeView>
    <Authorized>
        You are already logged in.
        <p>@User?.Profile?.Name</p>
        <p>@User?.Profile?.Email</p>
        <button @onclick="OnLogoutButtonClickAsync">Logout</button>
    </Authorized>
    <NotAuthorized>
        <button @onclick="OnLoginButtonClickAsync">Login</button>
    </NotAuthorized>
</AuthorizeView>

